				<TRAITEMENT ID="T1C1" condition="C1G2"
					DESCRIPTION="Soumission au transit">
					<SQL ID="SQL1" type="" traitement="T1C1"
						VALEUR="SELECT RCCODECREDITAIRE FROM ADMINISTRATEUR.SOUMISSIONCREDITAIRE
					WHERE RCCODEINDICATEUR='SOUTR' and RCCODECREDITAIRE='PARAMCODECREDITAIRE'
				   ">
