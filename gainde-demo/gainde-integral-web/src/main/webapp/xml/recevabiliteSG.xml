		<GROUPE ID="G1" fonction="F1"
			groupe="Controle lie au creditaire">
			<CONDITION ID="C1G1" groupe="G1" DATEVALEUR="01/01/06"
				DATEFIN="01/01/2031"
				VALEUR="SELECT * FROM ADMINISTRATEUR.INDICATEURREGDOU 
			WHERE RCTYPEREGIME='PARAMTYPEREGIME' and RCCODEREGIMEDOUANIER='PARAMCODEREGIMEDOUANE' and RCCODEINDICATEUR='YDCLCRE' and 
			RCDATEVALIDITE &lt;= 'PARAMDATETRAITEMENT' and (('PARAMDATETRAITEMENT' &lt;=RCDATECLOTURE ) or (RCDATECLOTURE IS NULL))">
				<TRAITEMENT ID="T1C1" condition="C1G1"
					DESCRIPTION="Existence creditaire">
					<SQL ID="SQL1" type="" traitement="T1C1"
						VALEUR="SELECT RCCODECREDITAIRE FROM ADMINISTRATEUR.CREDITAIRES
					WHERE RCCODECREDITAIRE='PARAMCODECREDITAIRE'
					or 'PARAMPPMDECLARANT' IN (SELECT RCCODEPPM FROM ADMINISTRATEUR.REGIMEDECLARANT
					WHERE RCTYPEREGIME='PARAMTYPEREGIME' and RCREGIMEDOUANIER='PARAMCODEREGIMEDOUANE' and RCCODEPPM='PARAMPPMDECLARANT')
				   ">
					</SQL>
					<MSG SQL="SQL1" traitement="T1C1"
						VALEUR="C1G1:Declarant ni creditaire, ni ambassade">
					</MSG>
				</TRAITEMENT>
			</CONDITION>
		</GROUPE>
